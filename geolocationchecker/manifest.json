{
    "manifest_version": 3,
    "name": "Geolocation Checker",
    "update_url": "https://github.com/joaocarias/jcf.lab.geolocationchecker/blob/main/updates.xml",
    "version": "1.0",
    "description": "Verifica a geolocalização do usuário.",
    "permissions": [
      "geolocation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
            "128": "icons/icon128.png"
      }
    }
  }
  